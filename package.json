{
  "name": "neuroexpert-app",
  "version": "3.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "start:render": "NODE_ENV=production node server.js",
    "build:render": "next build",
    "netlify": "next build",
    "export": "next build",
    "start:prod": "NODE_ENV=production node server.js",
    "build:standalone": "next build && cp -r public .next/standalone/public && cp -r .next/static .next/standalone/.next/static",
    "start:railway": "node server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:password": "node scripts/generate-password.js",
    "type-check": "tsc --noEmit",
    "test:ci": "jest --ci --coverage",
    "prepare": "husky install || true",
    "dev:all": "concurrently \"npm run dev\" \"npm run storybook\" \"npm run test:watch\"",
    "test:visual": "playwright test --ui",
    "test:visual:update": "playwright test --update-snapshots",
    "storybook": "storybook dev -p 6006",
    "check:all": "concurrently \"npm run lint\" \"npm run type-check\" \"npm run test\"",
    "fix:all": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@google/generative-ai": "^0.15.0",
    "@sentry/nextjs": "^10.5.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "critters": "^0.0.23",
    "date-fns": "^3.2.0",
    "framer-motion": "^10.18.0",
    "jsonwebtoken": "^9.0.2",
    "next": "14.2.32",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-chartjs-2": "^5.2.0",
    "styled-jsx": "^5.1.7",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^5.0.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
