{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "crons": [],
  "env": {
    "FORCE_CACHE_BUST": "2025-01-29-FORCE-DEPLOY-NOW",
    "DEPLOYMENT_TRIGGER": "FORCE_REBUILD_ALL_COMPONENTS"
  }
}