# AI Agents Configuration
# Скопируйте этот файл в .env.local и добавьте свои ключи

# OpenAI Configuration
OPENAI_API_KEY=sk-...your-openai-key-here...

# Anthropic Claude Configuration
ANTHROPIC_API_KEY=sk-ant-...your-anthropic-key-here...

# Google Gemini Configuration
GOOGLE_GEMINI_API_KEY=AIza...your-gemini-key-here...

# JWT и безопасность (обязательно для production)
JWT_SECRET=your-secret-key-here
ADMIN_PASSWORD_HASH=generated-hash-here

# Telegram уведомления (опционально)
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_CHAT_ID=your-telegram-chat-id

# База данных (опционально)
DATABASE_URL=postgresql://user:password@localhost:5432/dbname

# Настройки качества AI
MIN_QUALITY_SCORE=70
IMPROVEMENT_ENABLED=true

# Rate Limiting
AI_AGENT_RATE_LIMIT_WINDOW_MS=60000
AI_AGENT_RATE_LIMIT_MAX_REQUESTS=20